{
  "buildCommand": "git submodule update --init --recursive && npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "installCommand": "git submodule update --init --recursive && npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

